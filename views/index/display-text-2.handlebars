<div class="contain-fluid">
    <div class="display-text">
        <div class="row">
            <div>
                <img src="./images/logo-red.png" alt="Logo" class="logo">
            </div>
        </div>

        <div class="row">
            <div class="container-fluid">
                <div class="welcome-text card">
                    <p>{{ display.title }}</p>
                    <p>{{ display.subtitle }}</p>

                </div>
            </div>

            <div class="container-fluid center">
                <div class="desc-text card ">
                    <div style="padding-left: 50px">
                        <h3>
                            {{{ display.desc}}}
                        </h3>
                    </div>
                </div>
            </div>

            <div class="container-fluid">
                <div class="row">
                    <div class="display-text-footer center">
                        <div class="col s6 m6 l6">
                            <div class="col s12 m12 l12 btn-large disabled">
                                <h4>Time:</h4>
                            </div>
                            <div class="input-field col s12">
                                <h4>
                                    <input placeholder="Placeholder" id="meeting_rooms" type="text" class="validate"
                                        value="{{ display.footer}}">
                                </h4>
                            </div>
                        </div>
                        <div class="col s6 m6 l6">
                            <div class="col s12 m12 l12 btn-large disabled">
                                <h4>Meeting Rooms:</h4>
                            </div>

                            <div class="input-field col s12">
                                <h4>
                                    <input placeholder="Placeholder" id="meeting_rooms" type="text" class="validate"
                                        value="{{ display.subfooter}}">
                                </h4>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

        </div>

    </div>

</div>

</div>